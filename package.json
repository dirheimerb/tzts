{
	"name": "tzts",
	"version": "0.0.1",
	"description": "Timezone Typescript Library",
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./types": "./dist/index.d.ts"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.js",
	"scripts": {
		"build": "rollup -c",
		"test": "jest",
		"docs": "typedoc --out ./docs",
		"lint": "eslint ./src",
		"format": "prettier --write .",
		"prepare": "npm run build",
		"types": "tsc --emitDeclarationOnly"
	},
	"keywords": [
		"typescript",
		"timezones",
		"time",
		"timezone",
		"tzts",
		"tzts"
	],
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@jest/globals": "^29.7.0",
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-inject": "^5.0.5",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.7",
		"concurrently": "^8.2.2",
		"eslint": "^9.0.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"rollup": "^4.14.3",
		"rollup-plugin-banner": "^0.2.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-ts-paths": "^1.0.5",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typedoc": "^0.25.13",
		"typedoc-plugin-markdown": "4.0.0-next.54",
		"typedoc-plugin-mdn-links": "^3.1.21",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.7.0"
	},
	"author": {
		"name": "Bradley Dirheimer",
		"email": "b.dirheimer@gmail.com"
	},
	"homepage": "https://github.com/bradleydirheimer/tzts#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bradleydirheimer/tzts.git"
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"arrowParens": "always",
		"printWidth": 80,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSameLine": true,
		"semi": true,
		"singleAttributePerLine": true
	}
}
